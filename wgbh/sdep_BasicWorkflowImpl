<foxml:xmlContent>
<xsl:stylesheet xmlns:fedora-model="info:fedora/fedora-system:def/model#" xmlns:fedora-rels-ext="info:fedora/fedora-system:def/relations-external#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output method="xml" omit-xml-declaration="yes"></xsl:output>
  <xsl:param name="action">modifyObject</xsl:param>
  <xsl:template match="rdf:RDF">
    <process-definition>
      <xsl:apply-templates select="//fedora-rels-ext:hasWorkflow"></xsl:apply-templates>
      <xsl:apply-templates select="//fedora-model:hasModel"></xsl:apply-templates>
    </process-definition>
  </xsl:template>
  <xsl:template match="//fedora-model:hasModel">
    <xsl:variable name="parent_pid" select="substring-after(@rdf:resource, &apos;/&apos;)"></xsl:variable>
    <xsl:variable name="rels" select="document(concat(&apos;http://ovdev.wgbh.org:8180/fedora/get/&apos;, $parent_pid, &apos;/RELS-EXT&apos;))"></xsl:variable>
    <xsl:apply-templates mode="parent" select="$rels"></xsl:apply-templates>
  </xsl:template>
  <xsl:template match="rdf:RDF" mode="parent">
    <xsl:apply-templates select="//fedora-rels-ext:hasWorkflow"></xsl:apply-templates>
  </xsl:template>
  <xsl:template match="//fedora-rels-ext:hasWorkflow">
    <xsl:variable name="pid" select="substring-after(@rdf:resource, &apos;/&apos;)"></xsl:variable>
    <xsl:variable name="wkflw" select="document(concat(&apos;http://ovdev.wgbh.org:8180/fedora/get/&apos;, $pid, &apos;/Workflow&apos;))"></xsl:variable>
    <xsl:apply-templates select="$wkflw/workflows/process-definition[@name=$action]"></xsl:apply-templates>
    <xsl:apply-templates select="$wkflw/workflows/process-definition[@name=&apos;any&apos;]"></xsl:apply-templates>
  </xsl:template>
  <xsl:template match="//process-definition">
    <xsl:copy-of select="*"></xsl:copy-of>
  </xsl:template>
</xsl:stylesheet>
