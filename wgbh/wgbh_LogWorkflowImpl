<foxml:datastream ID="_workflow.xsl" STATE="A" CONTROL_GROUP="X" VERSIONABLE="true">
<foxml:datastreamVersion ID="_workflow.xsl.0" LABEL="" CREATED="2009-09-10T13:31:41.408Z" MIMETYPE="text/xml" SIZE="852">
<foxml:xmlContent>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output method="xml" omit-xml-declaration="yes"></xsl:output>
  <xsl:template match="/">
    <process-definition name="modifyDatastreamByValue">
      <sequence>
        <participant ref="fedora_log_prepare"></participant>
        <if>
          <equals field-value="dsID" other-value="File"></equals>
          <sequence>
            <participant ref="fedora_log_add_datastreams"></participant>
            <concurrent-iterator on-field="datastreams" to-field="datastream">
              <participant ref="fedora_add_datastream"></participant>
            </concurrent-iterator>
            <participant ref="fedora_activate_record"></participant>
          </sequence>
        </if>
      </sequence>
    </process-definition>
  </xsl:template>
</xsl:stylesheet>
</foxml:xmlContent>
</foxml:datastreamVersion>
